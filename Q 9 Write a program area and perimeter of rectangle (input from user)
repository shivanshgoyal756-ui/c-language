#include <stdio.h>

int main()
{
    float area,l,b,perimeter;
    printf("\nEnter length of rectangel...");
    scanf("%f",&l);
    printf("\nEnter breadht of rectangel...");
    scanf("%f",&b);
    area=l*b;
    printf("\nArea of rectangle  is%.2f\n",area);
    perimeter=2*(l+b);
    printf("\nPerimeter of rectangle is%.2f",perimeter);
    
    return 0;
}
